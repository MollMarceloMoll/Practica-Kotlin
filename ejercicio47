fun main() {
    fun suma (a: Int, b: Int): Int {
        return a + b
    }
    fun resta (a: Int, b: Int): Int {
        return a - b
    }
    fun multiplicacion (a: Int, b: Int): Int {
        return a * b
    }
    fun division (a: Int, b: Int): Int {
        return a / b
    }
    fun porcentaje (a: Int, b: Int): Int {
        return a % b
    }
    fun calculadora(e: String, a: Int, b: Int) {
        val s = "Suma"
        if(s == e) {
            println(suma(a,b))
        } else {
            println("Error")
        }
    }
    calculadora("Suma", 5, 5)
}
